{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "#lib/*": [
                "./src/lib/*"
            ]
        },
        "strict": true,
        "jsx": "react-jsx",
        "module": "ESNext",
        "target": "ESNext",
        "moduleResolution": "bundler",
        "skipLibCheck": true,
        "declaration": true,
        "declarationMap": true
    },
    "include": [
        "src"
    ]
}